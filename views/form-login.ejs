<!DOCTYPE html>
<html lang="es">

<!-- Head -->
<%- include('partials/head') %>
<body>
    <%- include('partials/header') %>
     
    <div id="login" class="login">
        <div class="container">
            <div class="row">                            
                <div class="col-md-12 shadow p-3 mb-5 bg-white rounded ">                                
                    <form class="form-horizontal" method="post">
                        <fieldset>
                            <legend class="text-center header">Login</legend>

                            <div class="form-group">
                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-user bigicon"></i></span>                                            
                                <div class="col-md-8 col-lg-10">
                                    <input id="fname" name="name" type="text" placeholder="Ingresá tu nombre" class="form-control">                                            
                                </div>
                            </div>
                                            
                            <div class="form-group">
                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-key fa-fw"></i></span>
                                <div class="col-md-8 col-lg-10">
                                    <input id="password" name="password" type="password" placeholder="Ingresá tu password" class="form-control">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12 text-center">
                                    <button type="submit" class="btn btn-primary btn-lg">Ingresar</button>
                                </div>
                            </div>       
                        </fieldset>
                    </form>
                </div>   
                           
            </div>
        </div>
    </div>

</body>    
</html>     